<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
    <extension
        point="org.ziptie.zap.hibernate.PersistenceUnit">
        <persistence-unit name="ziptie-ds">
            <class name="org.ziptie.addressing.AddressSet"/>
            <class name="org.ziptie.credentials.Credential"/>
            <class name="org.ziptie.credentials.CredentialConfig"/>
            <class name="org.ziptie.credentials.CredentialSet"/>
            <class name="org.ziptie.credentials.DeviceToCredentialSetMapping"/>
            <class name="org.ziptie.protocols.Protocol"/>
            <class name="org.ziptie.protocols.ProtocolConfig"/>
            <class name="org.ziptie.protocols.ProtocolProperty"/>
            <class name="org.ziptie.protocols.DeviceToProtocolMapping"/>
        </persistence-unit>
    </extension>
</plugin>
